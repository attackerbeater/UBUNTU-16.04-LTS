<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../../lib/internal/Magento/Framework/View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <head>
        <title>Example tab title</title>
    </head>
    <body>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">PAGE TITLE</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Magestudy\CustomerAccountTab\Block\Frontend\Example" name="example_tab" template="Magestudy_CustomerAccountTab::custom_tab_form.phtml" />
        </referenceContainer>
    </body>
</page>



