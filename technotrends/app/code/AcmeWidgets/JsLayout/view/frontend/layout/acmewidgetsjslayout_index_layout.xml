<?xml version="1.0"?>

<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
  <body>
    <referenceContainer name="content">
      <block class="AcmeWidgets\JsLayout\Block\Index" name="acmewidgets_jslayout_layout" template="AcmeWidgets_JsLayout::layout.phtml">
        <arguments>
          <argument name="jsLayout" xsi:type="array">
            <item name="components" xsi:type="array">
              <item name="testlayout" xsi:type="array">
                <item name="component" xsi:type="string">AcmeWidgets_JsLayout/js/layout</item>
              </item>
            </item>
          </argument>
        </arguments>
      </block>
    </referenceContainer>
  </body>
</page>
